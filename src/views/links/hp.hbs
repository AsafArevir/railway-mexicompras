<table class="table table-dark">
  <thead>
    <tr>
        <th scope="col">Date</th>
        <th scope="col">Category</th>
        <th scope="col">Name</th>
        <th scope="col">Description</th>
        <th scope="col">Price</th>
        <th scope="col">Amount</th>
        <th scope="col">Total</th>
    </tr>
  </thead>
  <tbody>
      {{#each linksp}}
    <tr>
        <th>{{fecha}}</th>
        <td>{{category}}</td>
        <td>{{nameps}}</td>
        <td>{{description}}</td>
        <td>{{precio}}</td>
        <td>{{cantidad}}</td>
        <td>{{total}}</td>
    </tr>
    {{/each}}
  </tbody>
</table>